<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CheckInReservation2">

    <TextView
        android:id="@+id/rrCod"
        android:layout_width="199sp"
        android:layout_height="42dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="68dp"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id8" />

    <TextView
        android:id="@+id/rrCid"
        android:layout_width="199sp"
        android:layout_height="42dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id6" />

    <TextView
        android:id="@+id/rrPax"
        android:layout_width="199sp"
        android:layout_height="42dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="108dp"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toStartOf="@+id/rrCod"
        app:layout_constraintTop_toBottomOf="@+id/rrType" />

    <TextView
        android:id="@+id/rrType"
        android:layout_width="199sp"
        android:layout_height="42dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="140dp"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toStartOf="@+id/id8"
        app:layout_constraintTop_toBottomOf="@+id/rrName" />

    <TextView
        android:id="@+id/rrName"
        android:layout_width="199sp"
        android:layout_height="42dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="124dp"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toStartOf="@+id/rrCid"
        app:layout_constraintTop_toBottomOf="@+id/rrId" />

    <TextView
        android:id="@+id/rrId"
        android:layout_width="279sp"
        android:layout_height="42dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="76dp"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/id6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/id8"
        android:layout_width="180dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="72dp"
        android:text="Check Out Date:"
        android:textAlignment="viewEnd"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rrCid" />

    <TextView
        android:id="@+id/id7"
        android:layout_width="139dp"
        android:layout_height="42dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:text="Pax:"
        android:textAlignment="viewEnd"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toStartOf="@+id/rrPax"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id4" />

    <TextView
        android:id="@+id/id6"
        android:layout_width="183dp"
        android:layout_height="37dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="84dp"
        android:text="Check In Date:"
        android:textAlignment="viewEnd"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/id4"
        android:layout_width="139dp"
        android:layout_height="42dp"
        android:layout_marginTop="24dp"
        android:text="Room Type:"
        android:textAlignment="viewEnd"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toStartOf="@+id/rrType"
        app:layout_constraintHorizontal_bias="0.005"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id3" />

    <TextView
        android:id="@+id/id3"
        android:layout_width="139dp"
        android:layout_height="42dp"
        android:layout_marginTop="20dp"
        android:text="Name:"
        android:textAlignment="viewEnd"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toStartOf="@+id/rrName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id2" />

    <TextView
        android:id="@+id/id2"
        android:layout_width="83dp"
        android:layout_height="37dp"
        android:layout_marginTop="16dp"
        android:text="Id:"
        android:textAlignment="viewEnd"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toStartOf="@+id/rrId"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="353dp"
        android:layout_height="44dp"
        android:layout_marginTop="16dp"
        android:text="Reservation Details"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/nxtBtn5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Next"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>