<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InspectMenu">

    <ImageView
        android:id="@+id/imgViewInspectRoom"
        android:layout_width="157dp"
        android:layout_height="141dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toTopOf="@+id/imgViewInspectFloor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.725"
        app:srcCompat="@drawable/btn_room" />

    <ImageView
        android:id="@+id/imgViewInspectFloor"
        android:layout_width="158dp"
        android:layout_height="144dp"
        android:layout_marginBottom="180dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/btn_floor"
        android:contentDescription="@string/todo" />

</androidx.constraintlayout.widget.ConstraintLayout>